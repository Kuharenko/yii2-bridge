{
    "name": "naffiq/yii2-bridge",
    "description": "Simple admin panel for enthusiasts",
    "type": "project",
    "require": {
        "yiisoft/yii2": "^2.0",
        "yii2tech/admin": "^1.0",
        "2amigos/yii2-tinymce-widget": "^1.1",
        "mongosoft/yii2-upload-behavior": "^0.1.7",
        "yiisoft/yii2-bootstrap": "^2.0",
        "kartik-v/yii2-widgets": "^3.4",
        "phpdocumentor/reflection-docblock": "^3.2.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "naffiq",
            "email": "naffiq@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "naffiq\\bridge\\": "",
            "naffiq\\bridge\\assets\\": "assets",
            "naffiq\\bridge\\controllers\\": "controllers",
            "naffiq\\bridge\\fixtures\\": "fixtures",
            "naffiq\\bridge\\modules\\query\\": "models/query",
            "naffiq\\bridge\\modules\\search\\": "models/search",
            "naffiq\\bridge\\modules\\": "models",
            "naffiq\\bridge\\widgets\\": "widgets",
            "naffiq\\bridge\\widgets\\columns\\": "widgets/columns",
            "naffiq\\bridge\\gii\\helpers\\": "gii/helpers",
            "naffiq\\bridge\\gii\\crud\\": "gii/crud",
            "naffiq\\bridge\\gii\\model\\": "gii/model"
        }
    },
    "require-dev": {
        "yiisoft/yii2-gii": "^2.0"
    }
}
